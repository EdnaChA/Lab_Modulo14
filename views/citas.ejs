<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Salud para Todos S.A.S</title>
    <link rel="stylesheet" href="/css/estilos.css" type="text/css"/>
</head>
<body>
    <header>    
        <%- include('menu') %>
    </header>
    <h2 id="first_title">Lista de citas médicas</h2>
    <section id = "tablas">
        <table id="tabla_citas">
            <thead>
              <tr>
                <th>Fecha</th>
                <th>Cédula del paciente</th>
                <th>Paciente</th>  
                <th>Especialidad</th> 
                <th>Doctor/a</th>
              </tr>
            </thead>
            <tbody id="datos_citas">
                <% consulta1.forEach(x =>{  %>
                    <tr>
                        <td><%= x.fecha %></td>
                        <td><%= x.cedula %></td>
                        <td><%= x.paciente %></td>
                        <td><%= x.especialidad %></td>
                        <td><%= x.doctor %></td>
                    </tr>
                <%})  %>
            </tbody>
        </table> 
    </section>
</body>
<script src="/js/forms.js"></script>
</html>